moon::ip tunnel add vti0 local 192.168.0.1 remote 192.168.0.2 mode vti key 0x01000201
moon::sysctl -w net.ipv4.conf.vti0.disable_policy=1
moon::ip link set vti0 up
moon::ip route add 10.2.0.0/16 dev vti0
sun::ip tunnel add ipsec0 local 192.168.0.2 remote 192.168.0.1 mode vti key 42
sun::sysctl -w net.ipv4.conf.ipsec0.disable_policy=1
sun::ip link set ipsec0 up
sun::ip route add 10.1.0.0/16 dev ipsec0
sun::ipsec start
moon::ipsec start
sun::expect-connection net-net
moon::expect-connection net-net
moon::ipsec up net-net
